{
  "name": "sender-bot",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production ts-node src/index.ts",
    "dev": "cross-env NODE_ENV=development node-dev src/index.ts",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate ./migrations/ -d ./src/Common/providers/ormProvider.ts",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run -d ./src/Common/providers/ormProvider.ts",
    "migration": "npm run migration:generate && npm run migration:run",
    "lint": "eslint \"{**/*,*}.{ts,tsx,js,jsx,json,mdx}\"",
    "lint:fix": "yarn run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cache-manager": "^4.0.2",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/uuid": "^8.3.4",
    "eslint": "8.22.0",
    "eslint-config-jave": "^1.2.0",
    "jest": "^29.0.3",
    "node-dev": "^7.4.3",
    "ts-jest": "^29.0.2",
    "tsconfig-paths-jest": "^0.0.1"
  },
  "dependencies": {
    "@depyronick/nestjs-clickhouse": "^1.0.16",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/typeorm": "^9.0.1",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "nest-winston": "^1.7.0",
    "nestjs-i18n": "^9.1.10",
    "nestjs-telegraf": "^2.6.1",
    "pg": "^8.8.0",
    "sha1": "^1.1.1",
    "sharp": "^0.31.0",
    "telegraf": "^4.9.1",
    "telegraf-i18n": "^6.6.0",
    "ts-node": "^10.9.1",
    "typegram": "^3.11.0",
    "typeorm": "^0.3.9",
    "typescript": "^4.8.3",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  }
